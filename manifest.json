{

  "manifest_version": 2,
  "name": "InstaSee",
  "version": "1.0",

  "description": "Lets you view instagram images without being logged in",

  "icons": {
    "48": "icon/instasee.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["scripts/main.js"]
    }
  ]

}